<body>
    <h1>Email Sender</h1>
    <button onclick="sendEmail()">Send Email</button>

    <script>
        function sendEmail() {
            var email = 'bonehead77755@gmail.com'; // Replace 'your@email.com' with the email address you want to send to
            var subject = 'hi';
            var body = 'test';

            window.location = 'mailto:' + email + '?subject=' + subject + '&body=' + body;
        }
    </script>
</body>

